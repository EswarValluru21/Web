        android:layout_height="wrap_content"
        android:layout_below="@+id/jas_name"
        android:layout_alignTop="@+id/jas_name"
        android:layout_marginStart="23dp"
        android:layout_marginTop="27dp"
        android:layout_toEndOf="@+id/jas_about"
